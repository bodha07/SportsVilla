    <script>
        $(document).ready(function(){
            $('#product_img').fileinput({
                'allowedFileExtensions': ['jpg', 'png', 'gif','jpeg']
            });
            
            $('#item').on("click",function(){
                (this).classList.add('active');
                document.getElementById('order').classList.remove('active');
                document.getElementById('user').classList.remove('active');
            });
            $('#user').on("click",function(){
                (this).classList.add('active');
                document.getElementById('item').classList.remove('active');
                document.getElementById('user').classList.remove('active');
            });
             $('#order').on("click",function(){
                (this).classList.add('active');
                document.getElementById('item').classList.remove('active');
                document.getElementById('order').classList.remove('active');
            });
            $('#act-sp-2').on("click",function(){
                document.getElementById('sp-2').style.display = "block";
            });
            $('#act-sp-3').on("click",function(){
                document.getElementById('sp-3').style.display = "block";
            });
            $('#act-sp-4').on("click",function(){
                document.getElementById('sp-4').style.display = "block";
            });
            $('#act-sp-5').on("click",function(){
                document.getElementById('sp-5').style.display = "block";
            });
            $('#act-sp-6').on("click",function(){
                document.getElementById('sp-6').style.display = "block";
            });
            $('#act-p-2').on("click",function(){
                document.getElementById('p-2').style.display = "block";
            });
            $('#act-p-3').on("click",function(){
                document.getElementById('p-3').style.display = "block";
            });
            $('#act-p-4').on("click",function(){
                document.getElementById('p-4').style.display = "block";
            });
            $('#act-p-5').on("click",function(){
                document.getElementById('p-5').style.display = "block";
            });
            $('#act-p-6').on("click",function(){
                document.getElementById('p-6').style.display = "block";
            });
            $('[data-toggle="tooltip"]').tooltip();
            $(".side-nav .collapse").on("hide.bs.collapse", function() {                   
            $(this).prev().find(".fa").eq(1).removeClass("fa-angle-right").addClass("fa-angle-down");
            });
            $('.side-nav .collapse').on("show.bs.collapse", function() {                        
            $(this).prev().find(".fa").eq(1).removeClass("fa-angle-down").addClass("fa-angle-right");        
            });
            if(document.getElementById('item').classList.contains('active')==1){
                $('#add-item-button').on("click",function(){
                    document.getElementById('item-page').style.display = "none";
                });
                $('#edit-item-button').on("click",function(){
                    document.getElementById('item').classList.add('active');
                    document.getElementById('order').classList.remove('active');
                    document.getElementById('user').classList.remove('active');
                    document.getElementById('item-page').style.display = "none";
                });
                $('#add-spec').on("click",function(){
                    document.getElementById('add-spec').classList.add('active');
                    document.getElementById('add-purpose').classList.remove('active');
                    document.getElementById('specification').style.display = "block";
                    document.getElementById('purpose').style.display = "none";
                });
                $('#add-purpose').on("click",function(){
                    document.getElementById('add-spec').classList.remove('active');
                    document.getElementById('add-purpose').classList.add('active');
                    document.getElementById('specification').style.display = "none";
                    document.getElementById('purpose').style.display = "block";
                });
            }
        });
    </script>
